    </Reference>
    <Reference Include="Vinorsoft.Core.Utils">
      <HintPath>$(SolutionDir)\Libs\netcoreapp2.2\Vinorsoft.Core.Utils.dll</HintPath>
    </Reference>

    <Reference Include="Vinorsoft.Notify.DTO">
      <HintPath>$(SolutionDir)\Libs\netcoreapp2.2\Vinorsoft.Notify.DTO.dll</HintPath>
    </Reference>
    <Reference Include="Vinorsoft.Notify.Entities">
      <HintPath>$(SolutionDir)\Libs\netcoreapp2.2\Vinorsoft.Notify.Entities.dll</HintPath>
    </Reference>
    <Reference Include="Vinorsoft.Notify.Interface">
      <HintPath>$(SolutionDir)\Libs\netcoreapp2.2\Vinorsoft.Notify.Interface.dll</HintPath>
