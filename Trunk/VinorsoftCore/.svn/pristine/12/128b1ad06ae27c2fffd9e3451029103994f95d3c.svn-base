using AutoMapper;
using KTApps.Core.NotificationService.Entities;
using KTApps.Core.Paging;

namespace KTApps.Core.App.Notifications.Models.AutoMapper
{
    public class NotificationAutoMapper : Profile
    {
        public NotificationAutoMapper()
        {
            CreateMap<CoreContentReplaces, CoreContentReplaceModel>().ReverseMap();
            CreateMap<CoreEmailPendings, CoreEmailPendingModel>().ReverseMap();
            CreateMap<CoreEmailSendLogs, CoreEmailSendLogModel>().ReverseMap();
            CreateMap<CoreFCMConfigurations, CoreFCMConfigurationModel>().ReverseMap();
            CreateMap<CoreFCMDeviceTokens, CoreFCMDeviceTokenModel>().ReverseMap();
            CreateMap<CoreFCMPendings, CoreFCMPendingModel>().ReverseMap();
            CreateMap<CoreNotificationStatus, CoreNotificationStatusModel>().ReverseMap();
            CreateMap<CoreNotificationTemplates, CoreNotificationTemplateModel>().ReverseMap();
            CreateMap<CoreNotificationTypes, CoreNotificationTypeModel>().ReverseMap();
            CreateMap<CoreSMSConfigurations, CoreSMSConfigurationModel>().ReverseMap();
            CreateMap<CoreSMSLogs, CoreSMSLogModel>().ReverseMap();
            CreateMap<CoreSMSPendings, CoreSMSPendingModel>().ReverseMap();
            CreateMap<CoreSMTPConfigurations, CoreSMTPConfigurationModel>().ReverseMap();


            CreateMap<PagedList<CoreContentReplaces>, PagedList<CoreContentReplaceModel>>().ReverseMap();
            CreateMap<PagedList<CoreEmailPendings>, PagedList<CoreEmailPendingModel>>().ReverseMap();
            CreateMap<PagedList<CoreEmailSendLogs>, PagedList<CoreEmailSendLogModel>>().ReverseMap();
            CreateMap<PagedList<CoreFCMConfigurations>, PagedList<CoreFCMConfigurationModel>>().ReverseMap();
            CreateMap<PagedList<CoreFCMDeviceTokens>, PagedList<CoreFCMDeviceTokenModel>>().ReverseMap();
            CreateMap<PagedList<CoreFCMPendings>, PagedList<CoreFCMPendingModel>>().ReverseMap();
            CreateMap<PagedList<CoreNotificationStatus>, PagedList<CoreNotificationStatusModel>>().ReverseMap();
            CreateMap<PagedList<CoreNotificationTemplates>, PagedList<CoreNotificationTemplateModel>>().ReverseMap();
            CreateMap<PagedList<CoreNotificationTypes>, PagedList<CoreNotificationTypeModel>>().ReverseMap();
            CreateMap<PagedList<CoreSMSConfigurations>, PagedList<CoreSMSConfigurationModel>>().ReverseMap();
            CreateMap<PagedList<CoreSMSLogs>, PagedList<CoreSMSLogModel>>().ReverseMap();
            CreateMap<PagedList<CoreSMSPendings>, PagedList<CoreSMSPendingModel>>().ReverseMap();
            CreateMap<PagedList<CoreSMTPConfigurations>, PagedList<CoreSMTPConfigurationModel>>().ReverseMap();

        }
    }
}
